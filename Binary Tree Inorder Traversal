# Given the root of a binary tree, return the inorder traversal of its nodes' values.

# Example: Input: root = [1,null,2,3] -> Output: [1,3,2]

class Solution:
    def inorderTraversal(self, root: Optional[TreeNode]) -> List[int]:
        res = []

        def inorder(root):
            if not root:
                return
            inorder(root.left)
            res.append(root.val)
            inorder(root.right)
        inorder(root)
        return res


